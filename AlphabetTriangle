import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		int n;
		char ch='A';
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		System.out.println("enter a number");
		for(int i=1;i<=n;i++)
		{
		  for(int j=1;j<=i;j++)
		    System.out.print(ch++);
		  
		   {
		      System.out.print("\n");
		   }
    	   	}
    }
}


